{
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "LogSettings": {
    "ApplicationName": "RocketOps",
    "MinimumLevel": "Information",
    "OutputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}",
    "LogDirectory": "Logs",
    "LogFileName": "rocketops-.log",
    "EnableConsoleLogging": true,
    "EnableFileLogging": true,
    "UseCompactJsonConsoleLogging": false,
    "UseCompactJsonFileLogging": false,
    "RollingInterval": "Day",
    "RetainedFileCountLimit": 31,
    "FileSizeLimitBytes": 10485760,
    "SeqUrl": "",
    "SeqApiKey": "",
    "EnableApplicationInsights": false,
    "LevelOverrides": {
      "RocketOps": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "System": "Warning"
    }
  },
  "Services": {
    "alerts": "http://alerts-service",
    "monitoring": "http://monitoring-service",
    "reporting": "http://reporting-service"
  }
}
